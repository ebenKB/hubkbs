<%- partial('_partials/head') %>
<body>
  <div id="container">
    <div id="wrap">
      <%- partial('_partials/header', null, {cache: !config.relative_link}) %>
      <div class="outer">
        <section id="main"><%- body %></section>
        <% if (theme.sidebar && theme.sidebar !== 'bottom'){ %>
          <%- partial('_partials/sidebar', null, {cache: !config.relative_link}) %>
        <% } %>
      </div>
      <%- partial('_partials/footer', null, {cache: !config.relative_link}) %>
    </div>
    <%- partial('_partials/mobile-nav', null, {cache: !config.relative_link}) %>
    <%- partial('_partials/after-footer') %>
  </div>
</body>
</html>